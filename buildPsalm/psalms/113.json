[{"la":["In \u00e9xitu Isra\u00ebl de \u00c6g\u00fdpto,","domus Iacob de p\u00f3pulo b\u00e1rbaro:"],"fr":""},{"la":["Facta est Iud\u01fda sanctific\u00e1tio eius,","Isra\u00ebl pot\u00e9stas eius."],"fr":""},{"la":["Mare vidit, et fugit:","Iord\u00e1nis conv\u00e9rsus est retr\u00f3rsum."],"fr":""},{"la":["Montes exsultav\u00e9runt ut ar\u00edetes,","et colles sicut agni \u00f3vium."],"fr":""},{"la":["Quid est tibi, mare, quod fug\u00edsti:","et tu, Iord\u00e1nis, quia conv\u00e9rsus es retr\u00f3rsum?"],"fr":""},{"la":["Montes, exsult\u00e1stis sicut ar\u00edetes,","et colles, sicut agni \u00f3vium."],"fr":""},{"la":["A f\u00e1cie D\u00f3mini mota est terra,","a f\u00e1cie Dei Iacob."],"fr":""},{"la":["Qui conv\u00e9rtit petram in stagna aqu\u00e1rum,","et rupem in fontes aqu\u00e1rum."],"fr":""},{"la":["Non nobis, D\u00f3mine, non nobis:","sed n\u00f3mini tuo da gl\u00f3riam."],"fr":""},{"la":["Super miseric\u00f3rdia tua, et verit\u00e1te tua:","nequ\u00e1ndo dicant gentes: Ubi est Deus e\u00f3rum?"],"fr":""},{"la":["Deus autem noster in c\u00e6lo:","\u00f3mnia qu\u00e6c\u00famque v\u00f3luit, fecit."],"fr":""},{"la":["Simul\u00e1cra g\u00e9ntium arg\u00e9ntum, et aurum,","\u00f3pera m\u00e1nuum h\u00f3minum."],"fr":""},{"la":["Os habent, et non loqu\u00e9ntur:","\u00f3culos habent, et non vid\u00e9bunt."],"fr":""},{"la":["Aures habent, et non \u00e1udient:","nares habent, et non odor\u00e1bunt."],"fr":""},{"la":["Manus habent, et non palp\u00e1bunt:","pedes habent, et non ambul\u00e1bunt:","non clam\u00e1bunt in g\u00fatture suo."],"fr":""},{"la":["S\u00edmiles illis fiant qui f\u00e1ciunt ea:","et omnes qui conf\u00eddunt in eis."],"fr":""},{"la":["Domus Isra\u00ebl sper\u00e1vit in D\u00f3mino:","adi\u00fator e\u00f3rum et prot\u00e9ctor e\u00f3rum est,"],"fr":""},{"la":["Domus Aaron sper\u00e1vit in D\u00f3mino:","adi\u00fator e\u00f3rum et prot\u00e9ctor e\u00f3rum est,"],"fr":""},{"la":["Qui timent D\u00f3minum, sperav\u00e9runt in D\u00f3mino:","adi\u00fator e\u00f3rum et prot\u00e9ctor e\u00f3rum est."],"fr":""},{"la":["D\u00f3minus memor fuit nostri:","et bened\u00edxit nobis:"],"fr":""},{"la":["Bened\u00edxit d\u00f3mui Isra\u00ebl:","bened\u00edxit d\u00f3mui Aaron."],"fr":""},{"la":["Bened\u00edxit \u00f3mnibus, qui timent D\u00f3minum,","pus\u00edllis cum mai\u00f3ribus."],"fr":""},{"la":["Ad\u00edciat D\u00f3minus super vos:","super vos, et super f\u00edlios vestros."],"fr":""},{"la":["Bened\u00edcti vos a D\u00f3mino,","qui fecit c\u00e6lum, et terram."],"fr":""},{"la":["C\u00e6lum c\u00e6li D\u00f3mino:","terram autem dedit f\u00edliis h\u00f3minum."],"fr":""},{"la":["Non m\u00f3rtui laud\u00e1bunt te, D\u00f3mine:","neque omnes, qui desc\u00e9ndunt in inf\u00e9rnum."],"fr":""},{"la":["Sed nos qui v\u00edvimus, bened\u00edcimus D\u00f3mino,","ex hoc nunc et usque in s\u01fdculum."],"fr":""}]